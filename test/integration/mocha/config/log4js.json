{ "mainConf":
  {
   "appenders": {
     "console": { "type": "console", "layout": {"type": "pattern", "pattern": "%[[%d] [%p] %c %z -%] %m"} }
   },
   "categories": {
     "default": { "appenders": ["console"], "level": "debug" }
   }
 },
 "httpConf": {
     "level": "debug",
     "format": ":remote-addr - :method :url HTTP/:http-version :status :content-length :referrer :user-agent"
 }
}
